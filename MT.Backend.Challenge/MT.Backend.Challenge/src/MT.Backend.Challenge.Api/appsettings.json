{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${ConnectionStrings__DefaultConnection}"
  },
  "QueueServerConfig": {
    "Host": "${QueueServerConfig__Host}",
    "Port": "${QueueServerConfig__Port}",
    "Username": "${QueueServerConfig__Username}",
    "Password": "${QueueServerConfig__Password}"
  },
  "ImageServiceConfig": {
    "CloudName": "${ImageServiceConfig__CloudName}",
    "ApiKey": "${ImageServiceConfig__ApiKey}",
    "ApiSecret": "${ImageServiceConfig__ApiSecret}"
  }

}
